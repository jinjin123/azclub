<div class="az-member-clinal">
  <div class="container">
    <div class="v-line"></div>
      {% for cont in variables %}
        {% for item in cont %}
          <div class="az-clinical_box col-sm-12 col-md-6 col-lg-4">
          <a href="{{ item.target }}">
                <div class="az-clinical_img">
                 <img src="{{ item.img }}">
                </div>
                <div class="az-clinical_body">
                  <div class="az-clinical_body_title">
{#                    {{ title }}#}
                    {{ item.title }}
                  </div>
                  <div class="az-clinical_body_desc">
                    {{ item.short_d}}
                  </div>
                  <div class="az-oclinical_time_bottom">
                    <span>{{ item.time }}</span>
                  </div>
                </div>
          </a>
    </div>
        {% endfor %}
      {% endfor %}
  </div>
</div>
